{
  "name": "@studiocms/ui",
  "version": "0.3.2",
  "description": "The UI library for StudioCMS. Includes the layouts & components we use to build StudioCMS.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/withstudiocms/ui.git"
  },
  "author": {
    "name": "Louis Escher",
    "url": "https://studiocms.dev"
  },
  "contributors": [
    "louisescher",
    "TheOtterlord",
    "Adammatthiesen",
    "JusticeMatthew"
  ],
  "keywords": [
    "withastro",
    "astro-ui-library",
    "astro-components",
    "studiocms",
    "ui-library",
    "astro-integration"
  ],
  "homepage": "https://ui.studiocms.dev",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist", "assets"
  ],
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "build-scripts build 'src/**/*.ts' --build-tsconfig",
    "dev": "build-scripts dev 'src/**/*.ts' --build-tsconfig"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./toolbar": {
      "types": "./dist/toolbar/index.d.ts",
      "default": "./dist/toolbar/index.js"
    },
    "./components": "./assets/components/index.ts",
    "./css/*": "./assets/css/*",
    "./utils/*": "./assets/utils/*",
    "./types": "./assets/types/index.ts"
  },
  "dependencies": {
    "@iconify-json/heroicons": "catalog:",
    "@iconify/utils": "catalog:",
    "astro-transition-event-polyfill": "^1.1.0",
    "pathe": "catalog:"
  },
  "peerDependencies": {
    "astro": "catalog:peers",
    "vite": "catalog:peers"
  },
  "devDependencies": {
    "typescript": "catalog:"
  }
}
